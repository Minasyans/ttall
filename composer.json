{
    "name": "minasyans/ttall",
    "description": "TTALL Preset For Laravel 7 and Up.",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur Minasyan",
            "email": "artminasyanartwork@gmail.com",
            "homepage": "https://github.com/Minasyans/ttall"
        }
    ],
    "homepage": "https://github.com/minasyans/ttall",
    "keywords": [
        "tailwind",
        "tailwindcss",
        "turbolinks",
        "alpine",
        "alpinejs",
        "laravel",
        "livewire",
        "tall",
        "ttall",
        "preset"
    ],
    "require": {
        "illuminate/support": "~7|~8",
        "livewire/livewire": "^2.5",
        "laravel/ui": "^3.3"
    },
    "scripts": {
        "format": "php-cs-fixer fix --path-mode=intersection --config=.php_cs ./",
        "analyse": "phpstan analyse"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6",
        "phpstan/phpstan": "^0.12.90",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Minasyans\\Ttall\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minasyans\\Ttall\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Minasyans\\Ttall\\TtallServiceProvider"
            ],
            "aliases": {
                "Ttall": "Minasyans\\Ttall\\Facades\\Ttall"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
